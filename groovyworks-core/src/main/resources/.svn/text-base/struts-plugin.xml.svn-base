<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
	    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<bean type="com.opensymphony.xwork2.ObjectFactory" name="groovy-spring" class="net.vitarara.groovyworks.GroovyStrutsSpringObjectFactory" />

	<!--  Make the Groovy Spring object factory the automatic default -->
	<constant name="struts.objectFactory" value="groovy-spring" />
</struts>
